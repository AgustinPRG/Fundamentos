#include <iostream>
#include <math.h>
using namespace std;

int main()
{
    float a,b,c,determinante,resultado1,resultado2;
    cout<<"Calculo de una funcion cuadratica"<<endl;
    cout<<"Ingrese el valor de a: ";
    cin>>a;
    cout<<"Ingrese el valor de b: ";
    cin>>b;
    cout<<"Ingrese el valor de c: ";
    cin>>c;
    determinante=sqrt(b*b-4*a*c);
    if  (determinante>=0)
        {
            resultado1=(-b+determinante)/(2*a);
            resultado2=(-b-determinante)/(2*a);
            cout<<"X1 es: "<< resultado1<<endl;
            cout<<"X2 es: "<< resultado2<<endl;
        }
    else 
        {
            cout<<"No se puede resolver: ";
        }

    
    return 0;
}
